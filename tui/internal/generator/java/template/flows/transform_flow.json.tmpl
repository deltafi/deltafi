{
  "name": "{{.PluginName}}-{{.ActionName}}",
  "type": "TRANSFORM",
  "description": "{{.PluginName}} {{.ClassName}} transform",
  "subscribe": [
    {
      "topic": "{{.PluginName}}-data"
    }
  ],
  "transformActions": [
    {
      "name": "{{.ClassName}}",
      "type": "{{.GroupID}}.actions.{{.ClassName}}"
    }
  ],
  "publish": {
    "matchingPolicy": "FIRST_MATCHING",
    "defaultRule": {
      "defaultBehavior": "ERROR"
    },
    "rules": [
      {
        "topic": "{{.PluginName}}-egress"
      }
    ]
  }
}

