{
  "name": "unarchive-and-merge-passthrough-transform",
  "type": "TRANSFORM",
  "description": "Unarchive, merge into a single file, and publish to passthrough",
  "transformActions": [
    {
      "name": "Unarchive",
      "type": "org.deltafi.core.action.archive.Unarchive"
    },
    {
      "name": "Merge",
      "type": "org.deltafi.core.action.merge.Merge",
      "parameters": {
        "mediaType": "application/octet-stream",
        "mergedFilename": "{{filename}}.merged"
      }
    }
  ],
  "subscribe": [
    {
      "topic": "unarchive-and-merge-passthrough"
    }
  ],
  "publish": {
    "rules": [
      {
        "topic": "passthrough"
      }
    ]
  }
}
