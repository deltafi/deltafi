{
  "name": "unarchive-and-decompress-passthrough-transform",
  "type": "TRANSFORM",
  "description": "Unarchive, decompress extracted files, and publish each to passthrough",
  "transformActions": [
    {
      "name": "Unarchive",
      "type": "org.deltafi.core.action.archive.Unarchive"
    },
    {
      "name": "Decompress",
      "type": "org.deltafi.core.action.compress.Decompress"
    },
    {
      "name": "SplitContent",
      "type": "org.deltafi.core.action.SplitContent"
    }
  ],
  "subscribe": [
    {
      "topic": "unarchive-and-decompress-passthrough"
    }
  ],
  "publish": {
    "rules": [
      {
        "topic": "passthrough"
      }
    ]
  }
}
