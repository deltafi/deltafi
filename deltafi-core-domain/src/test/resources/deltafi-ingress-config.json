{
  "name": "sample",
  "description": "Test Plan",
  "type": "json",
  "sourcePlugin": {
    "artifactId": "sample",
    "groupId": "org.group",
    "version": "1.0.0"
  },
  "transformActions": [
    {
      "name": "Utf8TransformAction",
      "type": "org.deltafi.core.domain.TestAction",
      "consumes": "json",
      "produces": "json-utf8"
    },
    {
      "name": "SampleTransformAction",
      "consumes": "json-utf8",
      "produces": "json-utf8-sample"
    }
  ],
  "loadAction": {
    "name": "SampleLoadAction",
    "consumes": "json-utf8-sample"
  }
}
