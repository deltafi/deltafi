{
  "name": "smoke-test-ingress",
  "type": "TIMED_INGRESS",
  "description": "Create smoke DeltaFiles",
  "timedIngressAction": {
    "name": "SmokeTestIngressAction",
    "type": "org.deltafi.core.action.SmokeTestIngressAction",
    "parameters": {
      "metadata": { "smoke":  "test" }
    }
  },
  "publishRules": {
    "matchingPolicy": "FIRST_MATCHING",
    "defaultRule": {"defaultBehavior": "FILTER"},
    "rules": [
      {"topics": ["smoke-transform"]}
    ]
  },
  "cronSchedule": "*/5 * * * * *"
}
