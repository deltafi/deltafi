        {
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "${database}"
          },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "WITH params AS (\n  SELECT\n    $__from::bigint AS start_time_ms,\n    $__to::bigint AS end_time_ms,\n    $__interval_ms::bigint AS interval_ms\n),\ndata AS (\n  SELECT\n    time_bucket(\n      (SELECT (interval_ms || ' milliseconds')::interval FROM params),\n      e.timestamp\n    ) AS time,\n    e.data_source,\n    COUNT(*) AS files\n  FROM ts_errors e\n  LEFT JOIN ts_annotations a ON a.entity_id = e.id AND a.key = '$annotation_key'\n  WHERE\n    e.timestamp >= to_timestamp((SELECT start_time_ms FROM params) / 1000)\n    AND e.timestamp < to_timestamp((SELECT end_time_ms FROM params) / 1000)\n    AND e.data_source IN ($datasources)\n    AND (\n      (a.value IS NOT NULL AND a.value IN ($annotation_value))\n      OR ('Not Present' IN ($annotation_value) AND a.value IS NULL)\n    )\n  GROUP BY 1, 2\n),\ntime_series AS (\n  SELECT generate_series(\n    time_bucket(\n      (SELECT (interval_ms || ' milliseconds')::interval FROM params),\n      to_timestamp((SELECT start_time_ms FROM params) / 1000)\n    ),\n    time_bucket(\n      (SELECT (interval_ms || ' milliseconds')::interval FROM params),\n      to_timestamp((SELECT end_time_ms FROM params) / 1000)\n    ),\n    (SELECT (interval_ms || ' milliseconds')::interval FROM params)\n  ) AS time\n),\nall_series AS (\n  SELECT DISTINCT data_source FROM data\n),\nall_combinations AS (\n  SELECT time, data_source\n  FROM time_series\n  CROSS JOIN all_series\n)\nSELECT \n  ac.time,\n  COALESCE(SUM(d.files), 0) AS errors\nFROM all_combinations ac\nLEFT JOIN data d ON ac.time = d.time AND ac.data_source = d.data_source\nGROUP BY 1\nORDER BY ac.time;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [
                  {
                    "name": "\"timestamp\"",
                    "type": "functionParameter"
                  }
                ],
                "type": "function"
              },
              {
                "parameters": [
                  {
                    "name": "data_source",
                    "type": "functionParameter"
                  }
                ],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "name": "data_source",
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          },
          "table": "ts_egresses"
        }
      ],
