    {
      "datasource": {
        "type": "graphite",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "fieldMinMax": false,
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 24,
        "w": 17,
        "x": 7,
        "y": 0
      },
      "id": 46,
      "interval": "10s",
      "maxDataPoints": 99999,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "percentChangeColorMode": "standard",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "/^Time \\(lastNotNull\\)$/",
          "limit": 100,
          "values": true
        },
        "showPercentChange": false,
        "text": {
          "titleSize": 22,
          "valueSize": 22
        },
        "textMode": "value_and_name",
        "wideLayout": true
      },
      "pluginVersion": "11.4.0",
      "targets": [
        {
          "datasource": {
            "type": "graphite",
            "uid": "${datasource}"
          },
          "refId": "A",
          "target": "aliasByTags(groupByTags(seriesByTag('name=stats_counts.files_from_source', 'dataSource!='), 'sum', 'dataSource'), 'dataSource')"
        }
      ],
      "title": "Last seen flow traffic",
      "transformations": [
        {
          "id": "labelsToFields",
          "options": {
            "keepLabels": [
              "dataSource"
            ],
            "mode": "columns"
          }
        },
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "filterByValue",
          "options": {
            "filters": [
              {
                "config": {
                  "id": "lowerOrEqual",
                  "options": {
                    "value": 0
                  }
                },
                "fieldName": "Value"
              }
            ],
            "match": "any",
            "type": "exclude"
          }
        },
        {
          "id": "groupBy",
          "options": {
            "fields": {
              "Time": {
                "aggregations": [
                  "lastNotNull"
                ],
                "operation": "aggregate"
              },
              "Value": {
                "aggregations": [
                  "lastNotNull"
                ]
              },
              "dataSource": {
                "aggregations": [],
                "operation": "groupby"
              },
              "ingressFlow": {
                "aggregations": [],
                "operation": "groupby"
              }
            }
          }
        },
        {
          "id": "sortBy",
          "options": {
            "fields": {},
            "sort": [
              {
                "desc": true,
                "field": "Time (lastNotNull)"
              }
            ]
          }
        }
      ],
      "type": "stat"
    }
  ],
  "preload": false,
